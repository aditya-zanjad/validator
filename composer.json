{
    "name": "aditya-zanjad/validator",
    "description": "This is simple dependency-free validation package written in pure PHP pretty much inspired by Laravel's built-in validator. That's all for now.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "aditya-zanjad",
            "email": "adityazanjad474@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "AdityaZanjad\\Validator\\": "src/"
        },
        "files": [
            "src/Utils/Arr.php",
            "src/Utils/Str.php",
            "src/Utils/Var.php",
            "src/Utils/Date.php",
            "src/Presets/validate.php",
            "src/Presets/validator.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev",
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpstan/phpstan": "2.1.x-dev",
        "vimeo/psalm": "^6.5"
    }
}
